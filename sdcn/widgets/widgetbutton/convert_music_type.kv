<ConvertMusicType>:
	size_hint: (None,None)
	size: (1,100)
	canvas.before:
		Color:
			rgba: (0.8,0.8,0.8,0.9)
		Rectangle:
			pos: self.pos
            size: self.size
	Label:
		id: workflow_header
		text: 'Convert Music Type'
		size_hint: (0.8999,0.5)
		canvas.before:
			Color:
				rgba: (0.5,0.5,0.5,1)
			Rectangle:
				pos: self.pos
            	size: self.size
        color: (0,0,0,1)
	Button:
		text: 'X'
		size_hint: (0.1,0.5)
		on_press: root.delete_in_workflow_layout(root)
	Spinner:
		id: type
		text: 'choose'
		values: ('.wav','.mp3','.ra','.au')
		size_hint: (1,0.5)
		size: (1,44)