<CompressFile>:
	size_hint: (None,None)
	size: (1,100)
	canvas.before:
        Rectangle:
        	source: '../sdcn/data/images/gray1.png'
            pos: self.pos
            size: self.size
	Label:
		id: workflow_header
		text: 'Compress File'
		size_hint:(0.89,0.5)
		canvas.before:
			Rectangle:
				source: '../sdcn/data/images/gray2.png'
				pos: self.pos
            	size: self.size
	Button:
		text: 'X'
		size_hint:(0.1,0.5)
		on_press: root.delete_in_workflow_layout(root)
	
	Spinner:
		id: type
		text: '.choose'
		values: ('.Zip','.XZip')
		size_hint: (1,0.5)
		size:  (1,44)
